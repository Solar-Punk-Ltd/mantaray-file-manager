{
  "name": "mantaray-file-manager",
  "version": "1.0.0",
  "description": "A file manager for uploading and managing files on Swarm using Mantaray.",
  "main": "dist/fileManager.js",
  "scripts": {
    "build": "tsc",
    "test": "node --loader ts-node/esm node_modules/jest/bin/jest.js",
    "test:coverage": "jest --coverage",
    "start": "npm run build && node dist/index.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run lint"
  },
  "keywords": [
    "swarm",
    "mantaray",
    "file-manager",
    "bee-js",
    "typescript"
  ],
  "author": "Nidish Ramakrishnan",
  "license": "MIT",
  "dependencies": {
    "@ethersphere/bee-js": "^8.3.1",
    "@solarpunkltd/mantaray-js": "1.1.0",
    "cafe-utility": "^21.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^8.55.0",
    "@jest/globals": "^29.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "ethers": "^6.13.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
